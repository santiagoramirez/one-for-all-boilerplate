{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "rulesDirectory": [
    "app-desktop",
    "app-mobile",
    "app-shared",
    "app-web",
    "server"
  ],
  "rules": {
    "member-access": true,
    "no-any": true,
    "no-unsafe-any": true,
    "object-literal-sort-keys": false
  }
}
